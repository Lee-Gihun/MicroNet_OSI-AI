{
    "dir": {
        "log": "./Log",
        "data": "./data",
        "model": "./mobilenetv2/models",
        "results": "./mobilenetv2/results"
    },
    "log": {
        "logger": "mobilenetv2",
        "fpath": "mobilenetv2.log"
    },
    "data": {
        "dataset": "cifar10",
        "batch_size": 64
    },
    "model": {
        "algo": "mobilenetv2",
        "param": {
            "n_class": 10,
            "input_size": 224,
            "width_mult": 1.,
            "input_channel": 32,
            "last_channel": 1280
        }
    },
    "train_test": {
        "num_epochs": 50,
        "criterion": "cross_entropy",
        "fpath": {
            "model": "mobilenetv2_model.pt"
        },    
        "train": {
            "param": {
                "optim": "sgd",
                "learning_rate": 0.01,
                "momentum": 0.9,
                "weight_decay": 5e-4
            }
        },
        "test": {
            "param": {
                "verbose": "top1 accuracy",
                "topk": 1
            }
        }
    },
    "plot_store": {
        "fpath": {
            "train_loss": "mobilenetv2_train_loss.pickle",
            "test_acc": "mobilenetv2_acc.pickle",
            "img": "mobilenetv2_train_test_graph.png"
        }
    }
}