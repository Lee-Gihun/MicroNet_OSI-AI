{
    "data": {
        "dataset": "cifar100",
        "batch_size": 128,
        "valid_size": 0,
        "fixed_valid": true,
        "autoaugment": true,
        "aug_policy": "cifar100",
        "root": "./data/cifar100/valid0_batch128_cifar100_policy"
    },    
    "criterion": {
        "algo": "label_smoothing",
        "param": {
            "classes": 100,
            "smoothing": 0.3
        }
    },
    "optimizer": {
        "algo": "sgd",
        "param": {
            "lr": 0.13,
            "weight_decay": 1e-5,
            "momentum": 0.9
        }
    },
    "scheduler": {
        "enabled": true,
        "type": "cosine",
        "param": {
            "T_max": 1200
        }
    },
    "trainhandler": {
        "seed": 1111,
        "device": "cuda:0",
        "path": "./results",
        "name": "baseline",
        "num_epochs": 1200,
        "mixup": {
            "enabled": true,
            "alpha": 1.0
        },
        "precision": 32
    },
    "model": {
        "param": {
            "activation": "swish",
            "activation_param": {
            },
            "ghost_bn": true,
            "avail_resource": 2,
            "resolution_coefficient": 1.4,
            "width_coefficient": 0.9,
            "depth_coefficient": 1.0,
            "image_size": 32,
            "num_classes": 100
        },
        "pretrained": {
            "enabled": true,
            "sparsity": 60,
            "fpath": "./results/trained_models/baseline_iterative_sparsity_60.00"
        },
        "prune": {
            "enabled": true,
            "weight_reset": false,
            "method": "weight",
            "sparsity": 64,
            "rounds": 2,
            "gradually": true,
            "norm": true,
            "num_epochs": 600,
            "optimizer": {
                "lr": 0.13,
                "weight_decay": 1e-5,
                "momentum": 0.9
            },
            "scheduler": {
                "T_max": 600
            }
        }
    },
    "early_exit": {
        "enabled": false,
        "name": "_early_exit",
        "param": {
            "final_channels": 136,
            "use_bias": false,
            "thres": 0.85,
            "blocks_idx": 4
        },
        "criterion": {
            "soft_label_smoothing": true
        },
        "optimizer": {
            "lr": 0.13,
            "weight_decay": 1e-5,
            "momentum": 0.9
        },
        "scheduler": {
            "T_max": 800
        },
        "num_epochs": 800,
        "pretrained": {
            "enabled": false,
            "sparsity": 0,
            "fpath": "./results/trained_models/baseline_iterative_sparsity_64.00_early_exit"
        },
        "prune": {
            "enabled": false,
            "weight_reset": false,
            "method": "weight",
            "sparsity": 20,
            "rounds": 2,
            "gradually": true,
            "norm": true,
            "num_epochs": 400,
            "optimizer": {
                "lr": 0.13,
                "weight_decay": 1e-5,
                "momentum": 0.9
            },
            "scheduler": {
                "T_max": 400
            }
        },
        "inspection": {
            "enabled": true,
            "grid" : 0.01
        }
    }
}