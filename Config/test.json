{
    "data": {
        "dataset": "cifar100",
        "batch_size": 128,
        "valid_size": 0,
        "fixed_valid": true,
        "autoaugment": true,
        "aug_policy": "cifar100",
        "root": "./data/cifar100/valid0_batch128_cifar100_policy"
    },    
    "criterion": {
        "algo": "label_smoothing",
        "param": {
            "classes": 100
        }
    },
    "optimizer": {
        "algo": "sgd",
        "param": {
            "lr": 0.13
        }
    },
    "scheduler": {
        "enabled": false
    },
    "trainhandler": {
        "seed": 1111,
        "device": "cuda:0",
        "path": "./results",
        "name": "final_checkpoint",
        "mixup": {
            "enabled": true,
            "alpha": 1.0
        },
        "precision": 32
