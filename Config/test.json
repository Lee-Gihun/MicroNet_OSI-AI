            "enabled": false
        }
    },
    "early_exit": {
        "enabled": true,
        "name": "",
        "param": {
            "final_channels": 136,
            "use_bias": false,
            "thres": 0.84,
            "blocks_idx": 4
        },
        "criterion": {
        },
        "optimizer": {
            "lr": 0.13
        },
        "scheduler": {
        },
        "pretrained": {
            "enabled": true,
            "sparsity": 50,
            "fpath": "./results/trained_models/baseline_iterative_sparsity_64.00_early_exit_iterative_sparsity_50.00"
        },
        "prune": {
            "enabled": false
        },
        "inspection": {
            "enabled": false
        }
    }
