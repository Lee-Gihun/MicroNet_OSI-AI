{
    "data": {
        "dataset": "cifar100",
        "batch_size": 128,
        "valid_size": 0,
        "fixed_valid": true,
        "autoaugment": true,
        "aug_policy": "cifar100",
        "root": "./data/cifar100/valid0_batch128_cifar100_policy"
    },
    "model": {
        "param": {
            "activation": "celu",
            "activation_param": {
                "alpha": 0.1
            },
            "ghost_bn": true,
            "avail_resource": 2,
            "resolution_coefficient": 1.4,
            "width_coefficient": 0.9,
            "depth_coefficient": 1.0,
            "image_size": 32,
            "num_classes": 100
        },
        "pretrained": {
            "enabled": false,
            "fpath": "./results/trained_models/baseline"
        },
        "prune": {
            "enabled": false,
            "weight_reset": false,
            "method": "weight",
            "sparsity": 40,
            "rounds": 1,
            "gradually": true,
            "norm": true,
            "num_epochs": 300,
            "optimizer": {
                "lr": 0.13,
                "weight_decay": 1e-5,
                "momentum": 0.9
            },
            "scheduler": {
                "T_max": 300
            }
        }
    },
    "criterion": {
        "algo": "label_smoothing",
        "param": {
            "classes": 100,
            "smoothing": 0.3
        }
    },
    "optimizer": {
        "algo": "sgd",
        "param": {
            "lr": 0.13,
            "weight_decay": 1e-5,
            "momentum": 0.9
        }
    },
    "scheduler": {
        "enabled": true,
        "type": "cosine",
        "param": {
            "T_max": 1200
        }
    },
    "trainhandler": {
        "device": "cuda:1",
        "path": "./results",
        "name": "baseline_celu",
        "mixup": {
            "enabled": true,
            "alpha": 1.0
        },
        "precision": 16,
        "train": {
            "num_epochs": 1200
        }
    }
}
